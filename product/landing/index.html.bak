    <script>
        // Initialize Lucide Icons
        lucide.createIcons();

        // Navigation scroll effect
        const nav = document.getElementById('nav');
        window.addEventListener('scroll', () => {
            if (window.scrollY > 50) {
                nav.classList.add('scrolled');
            } else {
                nav.classList.remove('scrolled');
            }
        });

        // Demo panel toggle
        const toggleBtns = document.querySelectorAll('.demo-toggle-btn');
        const panels = document.querySelectorAll('.demo-panel');

        toggleBtns.forEach(btn => {
            btn.addEventListener('click', () => {
                const panel = btn.dataset.panel;
                
                toggleBtns.forEach(b => b.classList.remove('active'));
                btn.classList.add('active');
                
                panels.forEach(p => {
                    if (p.id === `panel-${panel}`) {
                        p.classList.add('active');
                    } else {
                        p.classList.remove('active');
                    }
                });
            });
        });

        // Interactive demo steps
        const demoSteps = document.querySelectorAll('.demo-step');
        const previews = document.querySelectorAll('.demo-preview-content');
        let currentStep = 1;

        function showStep(stepNum) {
            demoSteps.forEach((step, i) => {
                const num = i + 1;
                step.classList.remove('active', 'completed');
                if (num < stepNum) {
                    step.classList.add('completed');
                } else if (num === stepNum) {
                    step.classList.add('active');
                }
            });

            previews.forEach((preview, i) => {
                preview.style.display = (i + 1 === stepNum) ? 'block' : 'none';
            });

            currentStep = stepNum;
        }

        demoSteps.forEach((step, i) => {
            step.addEventListener('click', () => {
                showStep(i + 1);
            });
        });

        // Upload zone interaction
        const uploadZone = document.getElementById('upload-zone');
        const uploadProgress = document.getElementById('upload-progress');

        if (uploadZone) {
            uploadZone.addEventListener('click', () => {
                if (!uploadZone.classList.contains('uploading')) {
                    uploadZone.classList.add('uploading');
                    let progress = 0;
                    const interval = setInterval(() => {
                        progress += 5;
                        uploadProgress.style.width = progress + '%';
                        if (progress >= 100) {
                            clearInterval(interval);
                            setTimeout(() => {
                                uploadZone.classList.remove('uploading');
                                uploadProgress.style.width = '0%';
                                showStep(2);
                            }, 300);
                        }
                    }, 50);
                }
            });
        }

        // Auto-advance demo (optional, subtle)
        let demoAutoplay = null;
        function startDemoAutoplay() {
            demoAutoplay = setInterval(() => {
                const nextStep = currentStep >= 4 ? 1 : currentStep + 1;
                showStep(nextStep);
            }, 4000);
        }

        function stopDemoAutoplay() {
            if (demoAutoplay) {
                clearInterval(demoAutoplay);
                demoAutoplay = null;
            }
        }

        // Start autoplay when demo section is visible
        const demoSection = document.getElementById('demo');
        const demoObserver = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    startDemoAutoplay();
                } else {
                    stopDemoAutoplay();
                }
            });
        }, { threshold: 0.3 });

        if (demoSection) {
            demoObserver.observe(demoSection);
        }

        // Stop autoplay on user interaction
        demoSteps.forEach(step => {
            step.addEventListener('click', stopDemoAutoplay);
        });

        // Billing toggle
        const billingOptions = document.querySelectorAll('.billing-option');
        const priceAmounts = document.querySelectorAll('.pricing-card-amount');
        const priceOriginals = document.querySelectorAll('.pricing-card-original');

        billingOptions.forEach(option => {
            option.addEventListener('click', () => {
                const billing = option.dataset.billing;
                
                billingOptions.forEach(o => o.classList.remove('active'));
                option.classList.add('active');

                priceAmounts.forEach(amount => {
                    // Only update if this card has dynamic pricing data
                    if (amount.dataset[billing]) {
                        amount.textContent = amount.dataset[billing];
                    }
                });

                priceOriginals.forEach(original => {
                    // Only update if this card has dynamic pricing data
                    if (original.dataset[billing] !== undefined) {
                        original.textContent = original.dataset[billing];
                    }
                });
            });
        });

        // FAQ Accordion
        const faqItems = document.querySelectorAll('.faq-item');

        faqItems.forEach(item => {
            const question = item.querySelector('.faq-question');
            question.addEventListener('click', () => {
                const isOpen = item.classList.contains('open');
                
                // Close all
                faqItems.forEach(i => i.classList.remove('open'));
                
                // Open clicked if it wasn't open
                if (!isOpen) {
                    item.classList.add('open');
                }
            });
        });

        // Smooth scroll for anchor links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    const navHeight = nav.offsetHeight;
                    const targetPosition = target.getBoundingClientRect().top + window.scrollY - navHeight - 20;
                    window.scrollTo({
                        top: targetPosition,
                        behavior: 'smooth'
                    });
                }
            });
        });

        // Add some entrance animations
        const animateOnScroll = () => {
            const elements = document.querySelectorAll('.feature, .testimonial, .pricing-card, .problem-point, .problem-stat');
            
            elements.forEach(el => {
                const rect = el.getBoundingClientRect();
                const isVisible = rect.top < window.innerHeight - 100;
                
                if (isVisible && !el.classList.contains('animated')) {
                    el.classList.add('animated');
                    el.style.animation = 'fadeInUp 0.6s ease forwards';
                }
            });
        };

        window.addEventListener('scroll', animateOnScroll);
        animateOnScroll(); // Run once on load
    </script>
